package FoodTracker;
import java.time.LocalDate;
import java.util.Scanner;
/****
 ***** Created by lucag on 11/11/2024
 ***** UPDATE PROGRAM DESCRIPTION HERE
 ****/
public class Food {

   private String name;
   private String type;
   private LocalDate useByDate;
   private  double cost;
   private int quantity;


   public Food(){


   }
   public Food(String itsName, String itsType, LocalDate itsUseByDate){
      name= itsName;
      type= itsType;
      useByDate= itsUseByDate;


   }
   public Food(String itsName, String itsType, LocalDate itsUseByDate,double itsCost ){
      name= itsName;
      type= itsType;
      useByDate= itsUseByDate;
      cost= itsCost;
   }
   public Food(String itsName, String itsType, LocalDate itsUseByDate,double itsCost, int itsQuantity ) {
      name = itsName;
      type = itsType;
      useByDate = itsUseByDate;
      cost = itsCost;
      quantity= itsQuantity;
   }






   public String getName() {
      return name;
   }// get method

  public String getType() {
     return type;
  }

   public static void main(String [] args){




   }//main
}//class   
